void addFast(){
    repeat(8){
        if(onBeeper()){
            moveForward();
            if(onBeeper()){
                moveForward();
                turnRight();
                moveForward();
                dropBeeper();
                turnAround();
                moveForward();
                turnRight();
                if(onBeeper()){
                    moveForward();
                    dropBeeper();
                    turnAround();
                    moveForward();
                    oneLeft();
                }
                else{
                    turnAround();
                    oneLeft();
                }
            }
            else{
                moveForward();
                if(onBeeper()){
                    turnRight();
                    moveForward();
                    dropBeeper();
                    turnAround();
                    moveForward();
                    turnLeft();
                    oneLeft();
                }
                else{
                    moveForward();
                    dropBeeper();
                    turnAround();
                    moveForward();
                    oneLeft();
                }
            }
        }
        else{
            moveForward();
            if(onBeeper()){
                moveForward();
                if(onBeeper()){
                    turnRight();
                    moveForward();
                    dropBeeper();
                    turnAround();
                    moveForward();
                    turnLeft();
                    oneLeft();
                }
                else{
                    moveForward();
                    dropBeeper();
                    turnAround();
                    moveForward();
                    oneLeft();
                }
            }
            else{
                moveForward();
                if(onBeeper()){
                    moveForward();
                    dropBeeper();
                    turnAround();
                    moveForward();
                    oneLeft();
                }
                else{
                    turnAround();
                    oneLeft();
                }
            }
        }
        
        
    }
    //letzte Runde
    if(onBeeper()){
        moveForward();
        if(onBeeper()){
            moveForward();
            turnRight();
            moveForward();
            dropBeeper();
            turnAround();
            moveForward();
            turnRight();
            if(onBeeper()){
                moveForward();
                dropBeeper();
                turnAround();
                moveForward();
                oneLeftLast();
            }
            else{
                turnAround();
                oneLeftLast();
            }
        }
        else{
            moveForward();
            if(onBeeper()){
                turnRight();
                moveForward();
                dropBeeper();
                turnAround();
                moveForward();
                turnLeft();
                oneLeftLast();
            }
            else{
                moveForward();
                dropBeeper();
                turnAround();
                moveForward();
                oneLeftLast();
            }
        }
    }
    else{
        moveForward();
        if(onBeeper()){
            moveForward();
            if(onBeeper()){
                turnRight();
                moveForward();
                dropBeeper();
                turnAround();
                moveForward();
                turnLeft();
                oneLeftLast();
            }
            else{
                moveForward();
                dropBeeper();
                turnAround();
                moveForward();
                oneLeftLast();
            }
        }
        else{
            moveForward();
            if(onBeeper()){
                moveForward();
                dropBeeper();
                turnAround();
                moveForward();
                oneLeftLast();
            }
            else{
                turnAround();
                oneLeftLast();
            }
        }
    }
    
}

void oneLeft(){
    moveForward();
    moveForward();
    turnLeft();
    moveForward();
    turnLeft();
}

void oneLeftLast(){
    moveForward();
    moveForward();
    turnLeft();
}

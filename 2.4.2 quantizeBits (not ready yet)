void quantizeBits(){
    repeat(9){
        turnLeft();
        repeat(4){
            moveForward();
        }
        if(beeperAhead()){
            while(frontIsClear()){
                moveForward();
                if(!onBeeper()){
                    dropBeeper();
                }
            }
            turnAround();
            while(frontIsClear()){
                moveForward();
            }
            turnLeft();
            moveForward();
        }
        else{
            turnAround();
            while(frontIsClear()){
                if(onBeeper()){
                    pickBeeper();
                    moveForward();
                }
                else{
                    moveForward();
                }
            }
            if(onBeeper()){
                pickBeeper();
                turnLeft();
                moveForward();
                
            }
            else{
                turnLeft();
                moveForward();
            }
        }
    }
    turnLeft();
    repeat(4){
        moveForward();
    }
    if(beeperAhead()){
        while(frontIsClear()){
            moveForward();
            if(!onBeeper()){
                dropBeeper();
            }
        }
        turnAround();
        while(frontIsClear()){
            moveForward();
        }
        turnLeft();
    }
    else{
        turnAround();
        while(frontIsClear()){
            if(onBeeper()){
                pickBeeper();
                moveForward();
            }
            else{
                moveForward();
            }
        }
        if(onBeeper()){
            pickBeeper();
            turnLeft();
            
        }
        else{
            turnLeft();
        }
    }
}

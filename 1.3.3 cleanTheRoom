void cleanTheRoom(){
    repeat(4){
        repeat(9){
            if(onBeeper()){
                pickBeeper();
            }
            moveForward();
        }
        turnLeft();
        if(onBeeper()){
            pickBeeper();
        }
        moveForward();
        turnLeft();
        repeat(9){
            if(onBeeper()){
                pickBeeper();
            }
            moveForward();
        }
        turnRight();
        if(onBeeper()){
            pickBeeper();
        }
        moveForward();
        turnRight();
    }
    repeat(9){
        if(onBeeper()){
            pickBeeper();
        }
        moveForward();
    }
    turnLeft();
    if(onBeeper()){
        pickBeeper();
    }
    moveForward();
    turnLeft();
    repeat(9){
        if(onBeeper()){
            pickBeeper();
        }
        moveForward();
    }
    if(onBeeper()){
        pickBeeper();
    }
    
}

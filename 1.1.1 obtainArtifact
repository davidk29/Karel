void obtainArtifact()
{
    turnRight();
    moveForward();
    turnLeft();
    moveForward();
    moveForward();
    moveForward();
    turnLeft();
    moveForward();
    pickBeeper();
    moveForward();
    turnLeft();
    moveForward();
    moveForward();
    moveForward();
    turnLeft();
    moveForward();
    dropBeeper();
}
